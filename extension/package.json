{
	"name": "wink-vscode-extension",
	"description": "VSCode part of a language server",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "0.0.1",
	"publisher": "vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/artrayme/WINK-vscode-extension"
	},
	"engines": {
		"vscode": "^1.26.0"
	},
	"contributes": {
		"commands": [
		  {
			"command": "scs.load",
			"title": "Load current SCs to the connected sc-machine",
			"category": "SCs"
		  },
		  {
			"command": "scs.unload",
			"title": "Delete current SCs from the connected sc-machine",
			"category": "SCs"
		  }
		]
	  },
	"scripts": {},
	"dependencies": {
		"vscode-languageclient": "^5.1.0-next.9",
		"ts-sc-client-ws": "0.3.0"

	},
	"devDependencies": {
		"@types/ws": "^6.0.0",
		"vscode": "^1.1.34"
	}
}
